{
  "name": "queencastle",
  "packageManager": "yarn@3.1.0",
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "serve": "parcel src/index.html",
    "prod": "parcel build src/index.html --no-source-maps --dist-dir prod --public-url ./",
    "watch-css": "parcel watch src/css/styles.css"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "autoprefixer": "latest",
    "babel-jest": "^27.3.1",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "parcel": "^2.0.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "picocolors": "^1.0.0",
    "postcss": "latest",
    "postcss-import": "^14.0.2",
    "tailwindcss": "latest"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^5.3.0",
    "d3-ease": "^3.0.1",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1"
  }
}
